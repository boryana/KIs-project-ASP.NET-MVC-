@model IEnumerable<New.Models.Grif>

@{
    ViewBag.Title = "Index";
}

<h2>Гриф за сигурност</h2>

<p>
    @Html.ActionLink("Създай нов", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GrifNames.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KIs.UniRegNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Laws.LawName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateExpired)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GrifNames.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KIs.UniRegNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Laws.LawName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateExpired)
        </td>
        <td>

            @Html.ActionLink("Промени", "Edit", new { id=item.GrifId }) |
            @Html.ActionLink("Details", "Details", new { id=item.GrifId }) |
            @Html.ActionLink("Изтрий", "Delete", new { id=item.GrifId })
        </td>
    </tr>
}

</table>
